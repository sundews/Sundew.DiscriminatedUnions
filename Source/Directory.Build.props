<Project>
    <PropertyGroup>
        <Deterministic>true</Deterministic>
        <Version>5.3</Version>
        <OutputPath>bin/$(Configuration)</OutputPath>
        <Nullable>enable</Nullable>
        <LangVersion>preview</LangVersion>
        <Company>Sundews</Company>
        <Authors>Kim Hugener-Ohlsen</Authors>
        <Copyright>Kim Hugener-Ohlsen</Copyright>
        <RepositoryUrl>https://github.com/sundews/Sundew.DiscriminatedUnions</RepositoryUrl>
        <PackageProjectUrl>https://github.com/sundews/Sundew.DiscriminatedUnions</PackageProjectUrl>
        <RepositoryType>git</RepositoryType>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
        <EnforceCodeStyleInBuild>true</EnforceCodeStyleInBuild>
    </PropertyGroup>
    <Import Project="Directory.Build.props.user" Condition="Exists('Directory.Build.props.user')"/>
</Project>